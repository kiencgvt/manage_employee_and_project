
<tr>
  <td><%= project.id %></td>
  <td><%= project.name %></td>
  <td><%= project.created_at.strftime("%r on %d/%m/%Y") %></td>
  <td align="center"><%= link_to "Delete", project, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %></td>
  <td align="center"><%= link_to "Edit", edit_project_path(project.id), class: "btn btn-primary" %></td>
  <td align="center"><%= link_to "Show", project_path(project.id), class: "btn btn-info"%></td>
</tr>


